  "dependencies": {
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "scripts": {
    "client-install": "yarn add --prefix client",
    "start": "node server.js",
